	bdsk-url-3 = {https://doi.org/10.1016/j.envsoft.2018.06.009}}

@article{befus2017,
	abstract = {Abstract Fresh groundwater discharge to coastal environments contributes to the physical and chemical conditions of coastal waters, but the role of coastal groundwater at regional to continental scales remains poorly defined due to diverse hydrologic conditions and the difficulty of tracking coastal groundwater flow paths through heterogeneous subsurface materials. We use three-dimensional groundwater flow models for the first time to calculate the magnitude and source areas of groundwater discharge from unconfined aquifers to coastal waterbodies along the entire eastern U.S. We find that 27.1 km3/yr (22.830.5 km3/yr) of groundwater directly enters eastern U.S. and Gulf of Mexico coastal waters. The contributing recharge areas comprised ~175,000 km2 of U.S. land area, extending several kilometers inland. This result provides new information on the land area that can supply natural and anthropogenic constituents to coastal waters via groundwater discharge, thereby defining the subterranean domain potentially affecting coastal chemical budgets and ecosystem processes.},
	author = {Befus, Kevin M. and Kroeger, Kevin D. and Smith, Christopher G. and Swarzenski, Peter W.},
	date-modified = {2022-08-18 14:40:08 -0500},
	doi = {10.1002/2017GL075238},
	eprint = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1002/2017GL075238},
	journal = {Geophysical Research Letters},
	keywords = {submarine groundwater discharge, coastal groundwater, coastal aquifer, groundwater modeling},
	number = {20},
	pages = {10,396-10,406},
	title = {{The Magnitude and Origin of Groundwater Discharge to Eastern U.S. and Gulf of Mexico Coastal Waters}},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1002/2017GL075238},
	volume = {44},
	year = {2017},
	bdsk-url-1 = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1002/2017GL075238},
	bdsk-url-2 = {10.1002/2017GL075238},
