	issn = {1364-8152},
	journal = {Environmental Modelling \& Software},
	keywords = {Computational reproducibility, Hydrologic modeling, MODFLOW, Metadata},
	pages = {217-229},
	title = {{Integrating scientific cyberinfrastructures to improve reproducibility in computational hydrology: Example for HydroShare and GeoTrust}},
	url = {https://www.sciencedirect.com/science/article/pii/S1364815217307880},
