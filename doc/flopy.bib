	journal = {Environmental Modelling \& Software},
	keywords = {FREEWAT, QGIS, MODFLOW, Free and Open Source Software, Groundwater management, ICT},
	pages = {210-230},
	title = {Integrating free and open source tools and distributed modelling codes in {GIS} environment for data-based groundwater management},
	url = {https://www.sciencedirect.com/science/article/pii/S1364815217309453},
	volume = {107},
	year = {2018},
